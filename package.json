	"scripts": {
		"test": "ng test",
		"generate-cert": "node local_env/generate-cert.js",
		"dev": "(trap 'kill 0' SIGINT; ionic serve --live-reload --no-open --configuration=development --address=app-dev.beaconcha.in & sleep 15; node local_env/run.js)",
		"dev-android": "ionic cap run android --livereload --external --no-open --configuration=development",
		"build-android": "npm run increment-build && ionic cap build android --prod --release --source-map=true",
		"build-ios": "npm run increment-build && ionic cap build ios --prod --release --source-map=true",
		"prettier-format": "prettier --config .prettierrc.yml 'src/**/*.{ts,html,scss}' --write",
		"prettier-check": "prettier -c --config .prettierrc.yml 'src/**/*.{ts,html,scss}'",
		"lint": "eslint .",
		"increment-build": "node increment-build.js",
		"build-bundle": "npm run increment-build && ionic build --prod --release && node seal-bundle.js"
	},
